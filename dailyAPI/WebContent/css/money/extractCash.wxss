* {
  margin: 0;
  padding: 0;
}
body,
div,
dl,
dt,
dd,
ul,
ol,
li,
h1,
h2,
h3,
h4,
h5,
h6,
pre,
code,
form,
fieldset,
legend,
input,
button,
textarea,
p,
blockquote,
th,
td {
  margin: 0;
  padding: 0;
  font-family: "PingFang SC";
}
ul li {
  list-style: none;
}
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
a {
  text-decoration: none;
}
.line {
  height: .2rem;
  background: #eee;
}
body {
  background: #f5f5f5;
}
.extractCashApply .balance {
  height: .7rem;
  background: #fff;
  font-size: .28rem;
  color: #999;
  line-height: .7rem;
  box-sizing: border-box;
  padding: 0 .3rem;
  border-bottom: .01rem solid #eee;
}
.extractCashApply .extractCash {
  background: #fff;
}
.extractCashApply .extractCash p {
  height: .6rem;
  line-height: .6rem;
  color: #333;
  font-size: .28rem;
  box-sizing: border-box;
  padding: 0 .3rem;
}
.extractCashApply .extractCash .inpCash {
  box-sizing: border-box;
  padding: 0 .3rem;
  font-size: 0;
  height: .8rem;
  line-height: .8rem;
}
.extractCashApply .extractCash .inpCash span {
  font-size: .12rem;
}
.extractCashApply .extractCash .inpCash input {
  width: 94%;
  height: .6rem;
  font-size: .38rem;
  border: none;
  outline: none;
  font-weight: 900;
}
.extractCashApply .cardInfo {
  margin-top: .1rem;
  background: #fff;
}
.extractCashApply .cardInfo > div {
  font-size: .24rem;
  display: flex;
  box-sizing: border-box;
  padding: 0 .3rem;
  align-items: center;
  height: .7rem;
  border-bottom: .01rem solid #eee;
}
.extractCashApply .cardInfo > div span {
  display: inline-block;
  width: 1.4rem;
}
.extractCashApply .cardInfo > div input {
  width: 80%;
  height: .6rem;
  border: none;
  outline: none;
}
.extractCashApply .bankName {
  background: #fff;
  box-sizing: border-box;
  padding: .2rem .3rem;
  margin-top: .1rem;
}
.extractCashApply .bankName p {
  height: .6rem;
  line-height: .6rem;
  color: #333;
  font-size: .28rem;
}
.extractCashApply .bankName input {
  width: 100%;
  height: .6rem;
  font-size: .32rem;
  border: none;
  outline: none;
  font-weight: 900;
}
.extractCashApply > p {
  font-size: .12rem;
  color: #999;
  box-sizing: border-box;
  padding: 2rem .3rem 0;
}
.extractCashApply button {
  display: block;
  width: 6.6rem;
  height: .7rem;
  text-align: center;
  background: #ff2c46;
  line-height: .7rem;
  color: #fff;
  border-radius: .04rem;
  margin: .4rem auto 0;
  border: none;
  font-size: .24rem;
  outline: none;
}
